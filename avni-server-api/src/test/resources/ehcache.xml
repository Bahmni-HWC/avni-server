<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true" monitoring="autodetect" dynamicConfig="true"
         maxBytesLocalHeap="32M">
    <diskStore path="user.home"/>
    <cache name="org.avni.server.domain.Concept"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
        <!--        <cacheEventListenerFactory class="org.avni.server.framework.hibernate.AvniCacheEventListenerFactory"  listenFor="all"/>-->
    </cache>
    <cache name="org.avni.server.domain.ConceptAnswer"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
        <!--        <cacheEventListenerFactory class="org.avni.server.framework.hibernate.AvniCacheEventListenerFactory"  listenFor="all"/>-->
    </cache>
    <cache name="org.avni.server.domain.User"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
        <!--        <cacheEventListenerFactory class="org.avni.server.framework.hibernate.AvniCacheEventListenerFactory"  listenFor="all"/>-->
    </cache>
    <cache name="org.avni.server.domain.AddressLevel"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
        <!--        <cacheEventListenerFactory class="org.avni.server.framework.hibernate.AvniCacheEventListenerFactory"  listenFor="all"/>-->
    </cache>
    <cache name="org.avni.server.domain.VirtualCatchment"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
        <!--        <cacheEventListenerFactory class="org.avni.server.framework.hibernate.AvniCacheEventListenerFactory"  listenFor="all"/>-->
    </cache>
    <defaultCache maxEntriesLocalHeap="0" eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
        <!--        <cacheEventListenerFactory class="org.avni.server.framework.hibernate.AvniCacheEventListenerFactory"  listenFor="all"/>-->
    </defaultCache>
</ehcache>
